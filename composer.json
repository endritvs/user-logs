{
    "name": "endritvs/user-logs",
    "description": "Track user logs easily with this package, easy also to integrate into all Laravel projects.",
    "type": "library",
    "license": "MIT", 
    "authors": [
        {
            "name": "Endrit Saiti",
            "email": "endritsaiti8@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0", 
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Endritvs\\UserLogs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Endritvs\\UserLogs\\UserLogsServiceProvider"
            ]
        }
    },
    "keywords": ["laravel", "logging", "user", "tracking"]
}
